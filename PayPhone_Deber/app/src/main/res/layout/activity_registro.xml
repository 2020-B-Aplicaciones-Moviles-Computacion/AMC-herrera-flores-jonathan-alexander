<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFAE15"
    tools:context=".Registro">

    <androidx.constraintlayout.utils.widget.MockView
        android:id="@+id/mockView3"
        android:layout_width="333dp"
        android:layout_height="555dp"
        android:layout_marginBottom="40dp"
        android:foreground="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintCircleRadius="500dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_enviarCodigo"
        android:layout_width="263dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="35dp"
        android:layout_marginRight="35dp"
        android:layout_marginBottom="82dp"
        android:text="Enviar"
        app:backgroundTint="#FF8D11"
        app:layout_constraintBottom_toBottomOf="@+id/mockView3"
        app:layout_constraintEnd_toEndOf="@+id/mockView3" />

    <Button
        android:id="@+id/btn_cancelarRegistro"
        android:layout_width="263dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="35dp"
        android:layout_marginRight="35dp"
        android:layout_marginBottom="25dp"
        android:text="Cancelar"
        app:backgroundTint="#707070"
        app:layout_constraintBottom_toBottomOf="@+id/mockView3"
        app:layout_constraintEnd_toEndOf="@+id/mockView3" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp"
        android:layout_marginBottom="144dp"
        android:text="Te enviaremos un código a tu teléfono"
        android:textColor="#FFA748"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/mockView3"
        app:layout_constraintEnd_toEndOf="@+id/mockView3" />

    <EditText
        android:id="@+id/txt_phoneOlv3"
        android:layout_width="180dp"
        android:layout_height="43dp"
        android:layout_marginTop="208dp"
        android:layout_marginEnd="112dp"
        android:layout_marginRight="112dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/mockView3"
        app:layout_constraintTop_toTopOf="@+id/mockView3" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginTop="89dp"
        android:layout_marginEnd="120dp"
        android:layout_marginRight="120dp"
        android:text="Ingresa tu código de país."
        android:textColor="#FFA748"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/mockView3"
        app:layout_constraintTop_toTopOf="@+id/mockView3" />

    <EditText
        android:id="@+id/txt_codPaisOlv3"
        android:layout_width="92dp"
        android:layout_height="40dp"
        android:layout_marginTop="119dp"
        android:layout_marginEnd="200dp"
        android:layout_marginRight="200dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="+593"
        app:layout_constraintEnd_toEndOf="@+id/mockView3"
        app:layout_constraintTop_toTopOf="@+id/mockView3" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginTop="175dp"
        android:layout_marginEnd="91dp"
        android:layout_marginRight="91dp"
        android:text="Ingresa tu número de teléfono"
        android:textColor="#FFA748"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/mockView3"
        app:layout_constraintTop_toTopOf="@+id/mockView3" />

    <androidx.constraintlayout.utils.widget.MockView
        android:id="@+id/mockView4"
        android:layout_width="0dp"
        android:layout_height="150dp"
        android:foreground="#FF8D11"
        app:layout_constraintBottom_toBottomOf="@+id/circularImageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <com.mikhaellopez.circularimageview.CircularImageView
        android:id="@+id/circularImageView2"
        android:layout_width="91dp"
        android:layout_height="112dp"
        android:layout_marginTop="104dp"
        android:src="@drawable/logo"
        app:civ_border_color="#3f51b5"
        app:civ_border_width="0dp"
        app:civ_shadow="true"
        app:civ_shadow_color="#000000"
        app:civ_shadow_radius="1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="0dp"
        android:layout_height="22dp"
        android:background="#FF8D11"
        android:text="Version 3.3.10 (235)"
        android:textColor="#FFF7EE"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>